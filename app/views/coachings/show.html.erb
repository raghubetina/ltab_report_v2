<div class="page-header">
  <h3>Coaching #<%= @coaching.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Team</dt>
      <dd><%= @coaching.team_id %></dd>

      <dt>User</dt>
      <dd><%= @coaching.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/coachings" class="btn btn-primary">
        Back
      </a>
      <% if current_user == @coaching.user %>
        <a href="/coachings/<%= @coaching.id %>/edit" class="btn btn-warning">
          Edit
        </a>
        <a href="/delete_coaching/<%= @coaching.id %>" class="btn btn-danger" rel="nofollow">
          Delete
        </a>
      <% end %>
    </div>
  </div>
</div>
